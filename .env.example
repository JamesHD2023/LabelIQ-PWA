# LabelIQ PWA Environment Configuration
# Copy this file to .env.local and fill in your API keys

# Google Cloud Vision API (for OCR text extraction)
REACT_APP_GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Anthropic Claude API (for AI ingredient analysis)
REACT_APP_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# USDA FoodData Central API (optional - free tier has no key requirement)
REACT_APP_USDA_API_KEY=your_usda_api_key_here

# Edamam Nutrition API (optional - for enhanced nutrition features)
REACT_APP_EDAMAM_APP_ID=your_edamam_app_id_here
REACT_APP_EDAMAM_APP_KEY=your_edamam_app_key_here

# App Configuration
REACT_APP_APP_NAME=LabelIQ
REACT_APP_VERSION=1.0.0
REACT_APP_API_BASE_URL=https://api.labeliq.app

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_SOCIAL_SHARING=true
REACT_APP_ENABLE_CALORIE_TRACKING=true
REACT_APP_ENABLE_ADDITIVE_DATABASE=true

# Development Settings
REACT_APP_DEBUG_MODE=false
REACT_APP_MOCK_API_RESPONSES=false